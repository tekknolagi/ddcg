BasedOnStyle: Google
AlignEscapedNewlinesLeft: false
DerivePointerAlignment: false
PointerAlignment: Left
IncludeBlocks: Regroup
IncludeCategories:
  # C system headers
  - Regex:           '^<.*\.h?>'
    Priority:        1
  # C++ standard library headers
  - Regex:           '^<.*'
    Priority:        2
  # Project headers
  - Regex:           '.*'
    Priority:        4
IncludeIsMainRegex: "(-linux)?(-darwin)?(-x64)?(-test)?$"
